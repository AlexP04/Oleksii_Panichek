class Main {
    public static void main(String[] args) {
        int[] array = {1, 3, 6, 2, 2, 0, 4, 5};
        int goal_number  = 5;
        int counter = 0;
        
        for(int i=0; i< array.length; i++){
            for(int j=i+1; j< array.length; j++){
                if(array[i]+array[j] == goal_number){
                    counter++;
                }
            }
        }
        
        System.out.println("Amount of pairs: "+counter);
    }
    
}
